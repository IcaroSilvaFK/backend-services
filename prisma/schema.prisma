// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Services {
  id          String   @id @default(cuid())
  title       String
  description String?
  value       Int
  comments    String?
  time        DateTime @default(now())

  @@map("services")
}
